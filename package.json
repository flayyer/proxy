{
  "name": "@flayyer/proxy",
  "version": "0.1.0",
  "description": "Helper function to proxy images via Flayyer network to bypass CORS",
  "keywords": [
    "flayyer",
    "image",
    "proxy",
    "typescript"
  ],
  "main": "dist/proxy.es5.js",
  "module": "dist/proxy.esm.js",
  "typings": "dist/proxy.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Patricio LÃ³pez Juri <patricio@flayyer.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/flayyer/proxy.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "prepublishOnly": "yarn run build",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@flayyer/eslint-config": "^1.1.3",
    "@types/jest": "^26.0.23",
    "eslint": "^7.29.0",
    "husky": "^4.3",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "rollup": "^2.52.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  }
}
