{
  "name": "@flyyer/proxy",
  "version": "2.0.0",
  "description": "Helper function to proxy images via Flyyer CDN network to bypass CORS",
  "keywords": [
    "flyyer",
    "image",
    "proxy",
    "typescript"
  ],
  "main": "dist/proxy.es5.js",
  "module": "dist/proxy.esm.js",
  "typings": "dist/proxy.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Patricio LÃ³pez Juri <patricio@flyyer.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/useflyyer/proxy.git"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "prepublishOnly": "yarn run build",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@flyyer/eslint-config": "^2.0.0",
    "@types/jest": "^27.0.1",
    "eslint": "^7.32.0",
    "husky": "^4.3",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  }
}
